{
	"boot-source": {
		"kernel_path": "./vmlinux-5.10.236",
		"boot_args": "console=ttyS0 reboot=k panic=1"
	},
	"drives": [
		{
			"path_on_host": "./ubuntu-18.04.ext4",
			"is_root_device": true
		}
	],
	"machine-config": {
		"vcpu_count": 4,
		"mem_size_mib": 8192,
		"track_dirty_page": false
	},
        "features": [
                {
                        "feature": "acpi",
                        "enable": true
                }
        ]
}
